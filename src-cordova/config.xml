<?xml version='1.0' encoding='utf-8'?>
<widget id="moe.eirteam.divine" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Divine</name>
    <description>NHentai client</description>
    <author email="eirexe123@gmail.com" href="https://ero-one.moe">
        EIRTeam
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="22" />
    <engine name="android" spec="^7.1.4" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
</widget>
